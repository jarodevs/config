{
    "[json]": {
        "editor.codeActionsOnSave": {
            "json.sort": true
        },
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.quickSuggestions": {
            "strings": true
        }
    },
    "[typescript]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": true,
            "source.sortImports": true
        },
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[yaml]": {
        "diffEditor.ignoreTrimWhitespace": false,
        "editor.autoIndent": "keep",
        "editor.formatOnSave": true,
        "editor.insertSpaces": true,
        "editor.quickSuggestions": {
            "comments": false,
            "other": true,
            "strings": true
        },
        "editor.tabSize": 2
    },
    "activitusbar.activeColour": "#615F61",
    "editor.codeActionsOnSave": {
        "source.fixall.eslint": true
    },
    "editor.fontFamily": "SF Mono, Monaco",
    "editor.fontSize": 16,
    "editor.fontWeight": "normal",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.lineNumbers": "relative",
    "editor.minimap.enabled": false,
    "editor.tabSize": 2,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.wordWrap": "on",
    "eslint.format.enable": true,
    "eslint.validate": [
        "typescript",
        "javascript",
        "json",
        "prisma"
    ],
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "git.autofetch": true,
    "git.confirmSync": false,
    "githubIssues.issueBranchTitle": "${issueNumber}-${sanitizedIssueTitle}",
    "githubIssues.queries": [
        {
            "label": "My Issues",
            "query": "default"
        },
        {
            "label": "Created Issues",
            "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
        },
        {
            "label": "Recent Issues",
            "query": "state:open repo:${owner}/${repository} sort:updated-desc"
        }
    ],
    "githubPullRequests.defaultMergeMethod": "squash",
    "githubPullRequests.notifications": "pullRequests",
    "gitlab.aiAssistedCodeSuggestions.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.mode.active": "zen",
    "html.format.wrapLineLength": 85,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "livePreview.defaultPreviewPath": "/coverage/lcov-report/index.html",
    "problems.decorations.enabled": false,
    "redhat.telemetry.enabled": true,
    "security.workspace.trust.untrustedFiles": "open",
    "shellformat.path": "/opt/homebrew/bin/shfmt",
    "sonarlint.pathToNodeExecutable": "/Users/ariagroult/.nvm/versions/node/v18.16.0/bin/node",
    "terminal.external.linuxExec": "kitty",
    "terminal.external.osxExec": "Kitty.app",
    "terminal.integrated.fontFamily": "MesloLGL Nerd Font",
    "terminal.integrated.fontSize": 14,
    "typescript.enablePromptUseWorkspaceTsdk": true,
    "typescript.preferGoToSourceDefinition": true,
    "typescript.suggest.completeFunctionCalls": true,
    "typescript.tsserver.log": "off",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "vscode-neovim.neovimInitVimPaths.darwin": "/Users/ariagroult/.vscode-oss/init.vim",
    "window.nativeTabs": true,
    "window.titleBarStyle": "native",
    "window.zoomLevel": -1,
    "workbench.activityBar.visible": true,
    "workbench.colorTheme": "MacOS Modern Dark - Xcode Civic",
    "workbench.editor.showIcons": false,
    "workbench.iconTheme": "Monokai Pro Icons",
    "workbench.startupEditor": "none"
}